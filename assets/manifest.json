{
  "manifest_version": 3,
  "name": "Alcompare Chrome",
  "description": "See how algorithms are influencing you",
  "version": "0.1",
  "action": {
    "default_popup": "default.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["content/google.js"]
    }
  ]
}
